# Copy this file to .env and fill in your values

# Environment Mode
# Valid values: local, staging, production
ENV_MODE=local

#DATABASE
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_SSL=false

# LLM Providers:
ANTHROPIC_API_KEY=

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION_NAME=

GROQ_API_KEY=

# OpenRouter Configuration (Unified API for multiple LLM providers)
OPENROUTER_API_KEY=
OR_SITE_URL=https://your-app-domain.com  # Optional: Used for request attribution
OR_APP_NAME=YourAppName  # Optional: Used for request attribution, defaults to "suna.so"
MODEL_TO_USE=openrouter/openai/gpt-4o  # Uncomment to use OpenRouter as default

# DATA APIS
RAPID_API_KEY=

# WEB SEARCH & CRAWL
TAVILY_API_KEY=

# Sandbox container provider:
DAYTONA_API_KEY=
DAYTONA_SERVER_URL=
DAYTONA_TARGET=
